    "build:cc": "pnpm content-collections build",
    "build:start": "pnpm run build && pnpm run start",
    "mastra": "mastra dev --dir ./src/server/mastra",
    "analyze": "ANALYZE=true pnpm run build",
    "format": "npx prettier --write .",
    "knip": "knip"
